<div>
    {% for stage in dungeon.stages %}

        {% if stage.isComplete %}
            <p style="text-decoration:line-through;">{{ stage.name }}</p>
        {% else %}
            <p>{{ stage.name }}</p>
        {% endif %}
        <table>
            <tbody>
            <tr>
                {% for monster in stage.monsters %}
                    <td>
                        {{ monster.name }}
                        {{ include('character/character-stats.html.twig', {character: monster}, with_context=false) }}
                    </td>
                {% endfor %}
            </tr>
            </tbody>
        </table>
    {% endfor %}
</div>
